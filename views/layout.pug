doctype html
html(lang="en")
    head
        meta(charset="utf-8")
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet" href="/index.css")
        title Banking
        link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous")
    body
        nav(class="navbar navbar-dark bg-dark")
            div(class="container-fluid")
                a(class="navbar-brand color" href="/") Banking
                block search
                form(action="/t/upload" method="POST" class="d-flex" enctype="multipart/form-data")
                    input(type="file" class="form-control me-2" name="csv")
                    button(type="submit" class="btn btn-outline-light") Upload
        div(class="container-md")
            block content

    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8" crossorigin="anonymous")